{
  "rules": {
    "events": {
      ".read": "auth != null",
      "list": {
        ".read": "auth != null",
        ".write": false
      },
      "$eventId": {
        "counters": {
          ".read": true,
          ".write": false
        },
        "meta": {
          ".read": "auth != null",
          ".write": false
        },
        "participants": {
          "index": {
            ".read": "auth != null",
            ".write": false
          },
          "changes": {
            ".read": "auth != null",
            ".write": false
          }
        },
        "search": {
          ".read": true,
          ".write": false
        },
        "changes": {
          ".read": "auth != null",
          ".write": false
        }
      }
    },
    "admins": {
      "list": {
        ".read": "auth != null",
        ".write": false
      }
    },
    "invites": {
      "list": {
        ".read": "auth != null",
        ".write": false
      }
    },
    "bulkUploads": {
      "$uid": {
        "progress": {
          ".read": "auth != null && auth.uid == $uid",
          ".write": false
        }
      }
    }
  }
}
